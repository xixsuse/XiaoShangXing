<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/w3"
    android:orientation="vertical">

    <include layout="@layout/util_normal_title">

    </include>

    <ListView
        android:id="@+id/list_nodisturb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y48"
        android:choiceMode="singleChoice"
        android:divider="@color/g1"
        android:dividerHeight="@dimen/y1"></ListView>

    <TextView
        style="@style/g0_14sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/x48"
        android:layout_marginTop="@dimen/y21"
