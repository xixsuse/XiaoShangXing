<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/w3"
    android:orientation="vertical">

    <include layout="@layout/util_normal_title">

    </include>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/g1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/w1"
        android:paddingStart="@dimen/x48">

        <com.xiaoshangxing.utils.customView.CustomSwipeListView
            android:id="@+id/blacklist_listView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:divider="@drawable/toolbar_divider"
            android:listSelector="@android:color/transparent"></com.xiaoshangxing.utils.customView.CustomSwipeListView>
    </LinearLayout>

</LinearLayout>