<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/w3"
    android:orientation="vertical">

    <include layout="@layout/util_normal_title_end_text">

    </include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y126"
        android:layout_marginTop="@dimen/y48"
        android:background="@color/w0">

        <EditText
            android:id="@+id/edittext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/x48"
            android:layout_marginStart="@dimen/x42"
            android:background="@color/transparent"
            android:maxLength="15"
            android:singleLine="true"
            android:textColor="@color/b0"
            android:textCursorDrawable="@drawable/cursor_blue4"
            android:textSize="14sp" />
    </RelativeLayout>


</LinearLayout>