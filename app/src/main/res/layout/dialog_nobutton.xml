<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x420"
    android:layout_height="@dimen/y305"
    android:background="@drawable/dialog_ciecular_b5"
    android:minHeight="@dimen/y100"
    android:orientation="vertical">

    <ImageView
        android:layout_width="@dimen/x120"
        android:layout_height="@dimen/y120"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/y60"
        android:src="@mipmap/icon_liuxin_select" />

    <TextView
        android:id="@+id/dialog_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/y60"
        android:layout_marginTop="@dimen/y36"
        android:text="已完成注册"
        android:textColor="@color/w0"
        android:textSize="13sp" />

</LinearLayout>