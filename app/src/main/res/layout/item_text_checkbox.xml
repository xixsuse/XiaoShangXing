<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_144"
        android:background="@color/w0">

        <TextView
            android:id="@+id/text"
            style="@style/black_16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingStart="@dimen/x48"
            android:text="@string/app_name" />

        <CheckBox
            android:id="@+id/checkbox"
            android:layout_width="@dimen/x58"
            android:layout_height="@dimen/y40"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/width_48"
            android:background="@drawable/disturb_selector"
            android:button="@null"
            android:clickable="false"
            android:enabled="false" />

    </RelativeLayout>
